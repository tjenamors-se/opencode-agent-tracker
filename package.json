{
  "name": "@tjenamors.se/opencode-agent-tracker",
  "version": "0.0.0-alpha-dev",
  "description": "High-performance agent tracking for OpenCode using LMDB",
  "license": "GPL-3.0-or-later",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./plugin": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "opencode": {
    "plugin": true,
    "name": "agent-tracker"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --watch --config jest.config.cjs",
    "test:coverage": "jest --coverage --config jest.config.cjs",
    "clean": "rm -rf dist coverage",
    "prepack": "npm run build",
    "setup-local": "node scripts/setup-local-teste.js",
    "test-local": "node test-local-setup.mjs"
  },
  "dependencies": {
    "lmdb": "^3.5.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-typescript": "^7.28.5",
    "@opencode-ai/plugin": "^1.2.10",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "opencode",
    "plugin",
    "agent",
    "tracking",
    "lmdb",
    "xp",
    "skill-points"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tjenamors/opencode-agent-tracker"
  }
}
